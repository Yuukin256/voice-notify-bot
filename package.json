{
  "name": "discordjs-bot-template",
  "author": "Yuukin256",
  "license": "MIT",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node src/index.ts",
    "generate": "prisma generate --schema=src/prisma/schema.prisma",
    "migrate:dev": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "migrate:deploy": "prisma migrate deploy --schema=src/prisma/schema.prisma",
    "studio": "prisma studio --schema=src/prisma/schema.prisma",
    "build": "tsc",
    "start": "node dist/index.js"
  },
  "dependencies": {
    "@prisma/client": "^3.0.2",
    "discord.js": "^13.1.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.24.1",
    "prettier": "^2.3.2",
    "prisma": "^3.0.2",
    "ts-node": "^10.2.1",
    "typescript": "^4.3.5"
  },
  "engines": {
    "node": ">=16.6.0"
  },
  "packageManager": "yarn"
}
